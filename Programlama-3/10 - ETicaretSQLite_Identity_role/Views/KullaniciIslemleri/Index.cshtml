@model List<AppUser>

    <div class="jumbotron mt-5">
        <h1>Kullanıcılar</h1>
    
        <table class="table table-hover"> 
            <thead>
                <th>E-Posta</th>
                <th>Açıklama</th>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Email</td>
                        <td>@item.Aciklama</td>
                    </tr>
                }
                    
            </tbody>
        </table>
    </div>